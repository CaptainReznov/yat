<h2 class="panel-header">Nutrients</h2>

<div class="panel-stat">
  <div class="panel-stat-primary">
    <span class="panel-stat-type">Carbs:</span> <span class="panel-stat-amount">{{weight-measurement data.nutrients.carbs}}</span>
  </div>

  <div class="panel-stat-primary">
    <span class="panel-stat-type">Fat:</span> <span class="panel-stat-amount">{{weight-measurement data.nutrients.fat}}</span>
  </div>

  <div class="panel-stat-primary">
    <span class="panel-stat-type">Minterals:</span> <span class="panel-stat-amount">{{weight-measurement data.nutrients.minerals}}</span>
  </div>

  <div class="panel-stat-primary">
    <span class="panel-stat-type">Protein:</span> <span class="panel-stat-amount">{{weight-measurement data.nutrients.protein}}</span>
  </div>
</div>
